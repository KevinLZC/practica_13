<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="/assets/style.css" type="text/css" rel="stylesheet" />
    <title>TABLA DE PERSONAS</title>
</head>
<body>
    <table>
        <tr>
            <th>id</th>
            <th>Nombre</th>
            <th>Apellido</th>
            <th>Edad</th>
        </tr>
        <% data.forEach((fila) => { %>
            <tr>
                <td><%= fila.id %></td>
                <td><%= fila.nombre %></td>
                <td><%= fila.apellido %></td>
                <td><%= fila.edad %></td>
            </tr>
        <% }) %>
    </table>
</body>
</html>
